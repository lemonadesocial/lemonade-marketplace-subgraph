{
  "name": "lemonade-marketplace-subgraph",
  "version": "1.0.0",
  "scripts": {
    "prepare:testnet": "ln -fs subgraph.testnet.yaml subgraph.yaml",
    "prepare:mainnet": "ln -fs subgraph.mainnet.yaml subgraph.yaml",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy:testnet": "yarn prepare:testnet && graph deploy --node https://api.studio.thegraph.com/deploy/ lemonade-marketplace-testnet",
    "deploy:testnet:legacy": "yarn prepare:testnet && graph deploy --product hosted-service lemonadesocial/lemonade-marketplace-testnet",
    "deploy:mainnet": "yarn prepare:mainnet && graph deploy --node https://api.studio.thegraph.com/deploy/ lemonade-marketplace-mainnet",
    "deploy:mainnet:legacy": "yarn prepare:mainnet && graph deploy --product hosted-service lemonadesocial/lemonade-marketplace-mainnet",
    "create-local": "graph create --node http://localhost:8020/ lemonade-marketplace",
    "remove-local": "graph remove --node http://localhost:8020/ lemonade-marketplace",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 lemonade-marketplace"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.22.3",
    "@graphprotocol/graph-ts": "^0.22.1"
  },
  "devDependencies": {
    "standard-version": "^9.3.2"
  }
}
